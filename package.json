{
    "name": "shopware-data-generator",
    "description": "A tool to create AI-generated demo data for Shopware",
    "author": "Philipp Schuch",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "main": "dist/main.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npm run format && tsc --build",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "generate": "node --env-file=.env dist/main.js",
        "server": "node --env-file=.env dist/server.js"
    },
    "dependencies": {
        "openai": "4.92.1",
        "axios": "1.8.4",
        "express": "4.21.2",
        "zod": "3.24.2",
        "cheerio": "1.0.0-rc.12",
        "puppeteer-extra": "3.3.6",
        "puppeteer-extra-plugin-stealth": "2.11.2",
        "puppeteer": "23.11.1"
    },
    "devDependencies": {
        "typescript": "^5.8.2",
        "prettier": "^3.5.3",
        "@types/express": "^5.0.0",
        "@types/node": "^22.13.10",
        "@arethetypeswrong/cli": "^0.17.4"
    }
}
